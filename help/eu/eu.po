msgid "<gui>Contact List</gui> window"
msgstr "<gui>Kontaktu-zerrenda</gui> leihoa"

#: C/introduction.page:40(desc)
msgid "<app>Empathy</app> main window"
msgstr "<app>Empathy</app> leiho nagusia"

#: C/introduction.page:42(p)
