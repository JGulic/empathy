#: ../data/empathy.desktop.in.in.h:1
msgid "Empathy IM Client"
msgstr "Empathy IM କ୍ଲାଏଣ୍ଟ"

#: ../data/empathy.desktop.in.in.h:2
#| msgid "Send and receive instant messages"
