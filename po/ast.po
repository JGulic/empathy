msgid "Unknown reason"
msgstr "Razón desconocía"

#: ../libempathy/empathy-utils.c:222
msgid "Available"
msgstr "Disponible"

#: ../libempathy/empathy-utils.c:224
msgid "Busy"
msgstr "Ocupáu"

#: ../libempathy/empathy-utils.c:227
msgid "Away"
msgstr "Ausente"

#: ../libempathy/empathy-utils.c:229
msgid "Invisible"
