msgstr "Esu"

#: ../libempathy/empathy-utils.c:234
msgid "Busy"
msgstr "Užsiėmęs (-usi)"

#: ../libempathy/empathy-utils.c:237
msgid "Away"
msgstr "Pasitraukęs (-usi)"

#: ../libempathy/empathy-utils.c:239
msgid "Invisible"
